<head>
    <meta charset="utf-8" />
    <title>@{var title = ViewBag.pagetitle ?? "CBT";}@title</title>
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />

    <!-- ================== BEGIN BASE CSS STYLE ================== -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet">
    <link href="~/Scripts/assets/plugins/jquery-ui/themes/base/minified/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/Scripts/assets/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Scripts/assets/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Scripts/assets/css/animate.min.css" rel="stylesheet" />
    <link href="~/Scripts/assets/css/style.min.css" rel="stylesheet" />
    <link href="~/Scripts/assets/css/style-responsive.min.css" rel="stylesheet" />
    <link href="~/Scripts/assets/css/theme/default.css" rel="stylesheet" id="theme" />
    <!-- ================== END BASE CSS STYLE ================== -->
    <!-- ================== BEGIN BASE JS ================== -->
    <script src="~/Scripts/assets/plugins/pace/pace.min.js"></script>
    @*@Styles.Render("~/Content/css")*@
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("css", required: false)
    <!-- ================== END BASE JS ================== -->
</head>
<body>
    <!-- begin #page-loader -->
    <div id="page-loader" class="fade in"><span class="spinner"></span></div>
    <!-- end #page-loader -->
    <!-- begin #page-container -->
    <div id="page-container" class="page-container fade page-without-sidebar page-header-fixed page-with-top-menu">
        <!-- begin #header -->
        <div id="header" class="header navbar navbar-default navbar-fixed-top">
            <!-- begin container-fluid -->
            <div class="container-fluid">
                <!-- begin mobile sidebar expand / collapse button -->
                <div class="navbar-header">
                    <a href="index-2.html" class="navbar-brand"> CBT</a>
                    <!-- <button type="button" class="navbar-toggle" data-click="top-menu-toggled">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button> -->
                </div>
                <!-- end mobile sidebar expand / collapse button -->
                <!-- begin header navigation right -->
                @Html.Partial("_LoginPartial")
                <!-- end header navigation right -->
            </div>
            <!-- end container-fluid -->
        </div>
        <!-- end #header -->
        <!-- begin #top-menu -->
        <div id="top-menu" class="top-menu">
            <!-- begin top-menu nav -->
            <ul class="nav" style="text-align: center">
                @if (User.Identity.Name == "admin@example.com")
                {
                    <li class="has-sub">
                        <a href="@Url.Action("Index", "admin")">
                            <b class="caret pull-right"></b>
                            <!-- <i class="fa fa-key"></i> -->
                            <span>All Courses</span>
                        </a>

                    </li>

                    <li class="has-sub">
                        <a href="@Url.Action("addquestion", "admin")">
                            <b class="caret pull-right"></b>
                            <!-- <i class="fa fa-key"></i> -->
                            <span>Add Question</span>
                        </a>

                    </li>

                    @*<li class="has-sub">
                        <a href="@Url.Action("","")">
                            <b class="caret pull-right"></b>
                            
                            <span></span>
                        </a>

                    </li>*@

                }
                else
                {
                    <li class="has-sub">
                        <a href="@Url.Action("registercourse", "home")">
                            <b class="caret pull-right"></b>
                            <!-- <i class="fa fa-key"></i> -->
                            <span>Register Course</span>
                        </a>

                    </li>
                    <li class="has-sub">
                        <a href="@Url.Action("myexam", "exam")">
                            <b class="caret pull-right"></b>
                            <!-- <i class="fa fa-key"></i> -->
                            <span>Take Exam</span>
                        </a>

                    </li>
                }


                @*<li class="menu-control menu-control-left">
                    <a href="#" data-click="prev-menu"><i class="fa fa-angle-left"></i></a>
                </li>
                <li class="menu-control menu-control-right">
                    <a href="#" data-click="next-menu"><i class="fa fa-angle-right"></i></a>
                </li>*@
            </ul>
            <!-- end top-menu nav -->
        </div>
        <!-- end #top-menu -->
        <!-- begin #content -->
        <div id="content" class="content">
            <!-- begin breadcrumb -->
            <!-- end breadcrumb -->
            <!-- begin page-header -->
            <!-- end page-header -->

            <div class="row">
                <div class="col-md-12">
                    <div class="panel panel-inverse">
                        <div class="panel-heading">
                            @*<div class="panel-heading-btn">
                                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-default" data-click="panel-expand"><i class="fa fa-expand"></i></a>
                                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-repeat"></i></a>
                                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-warning" data-click="panel-collapse"><i class="fa fa-minus"></i></a>
                                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-danger" data-click="panel-remove"><i class="fa fa-times"></i></a>
                                </div>*@
                            <h4 class="panel-title">@title</h4>
                        </div>
                        <div class="panel-body">
                            @if (ViewBag.error != null || ViewBag.success != null)
                            {
                                <div class="alert alert-danger alert-dismissable" id="overload">
                                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                                    <span>@{string message = ViewBag.Error ?? ViewBag.success;}@message</span>
                                </div>
                             }
                           

                            @RenderBody()
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- end #content -->
        <!-- begin theme-panel -->
        <!-- end theme-panel -->
        <!-- begin scroll to top btn -->
        <a href="javascript:;" class="btn btn-icon btn-circle btn-success btn-scroll-to-top fade" data-click="scroll-top"><i class="fa fa-angle-up"></i></a>
        <!-- end scroll to top btn -->
    </div>
    <!-- end page container -->
    <!-- ================== BEGIN BASE JS ================== -->
    <script src="~/Scripts/assets/plugins/jquery/jquery-1.9.1.min.js"></script>
    <script src="~/Scripts/assets/plugins/jquery/jquery-migrate-1.1.0.min.js"></script>
    <script src="~/Scripts/assets/plugins/jquery-ui/ui/minified/jquery-ui.min.js"></script>
    <script src="~/Scripts/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
    <!--[if lt IE 9]>
        <script src="assets/crossbrowserjs/html5shiv.js"></script>
        <script src="assets/crossbrowserjs/respond.min.js"></script>
        <script src="assets/crossbrowserjs/excanvas.min.js"></script>
    <![endif]-->
    <script src="~/Scripts/assets/plugins/slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/Scripts/assets/plugins/jquery-cookie/jquery.cookie.js"></script>
    <!-- ================== END BASE JS ================== -->
    <!-- ================== BEGIN PAGE LEVEL JS ================== -->
    <script src="~/Scripts/assets/js/apps.min.js"></script>
    @RenderSection("scripts", required: false)
    <!-- ================== END PAGE LEVEL JS ================== -->

    <script>
        $(document).ready(function () {
            App.init();
        });
    </script>
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
