@*@model CBT_APP.Models.CourseViewModel*@

<div>
    <a href="#modal-dialog" class="btn btn-success pull-right" id="preview" data-toggle="modal">Preview</a>
    <br />
</div>


<div class="modal fade" id="modal-dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title">Course preview</h4>
            </div>
            <div class="modal-body" style="clear:both">
                <div id="main">
                    <table id="" class="table table-striped table-bordered">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>Title</th>
                                <th>Code</th>
                            </tr>
                        </thead>
                        <tbody id="crctbody">

                          
                        </tbody>
                    </table>
                    <form id="courseform" action="/home/RegisterCourse" method="post">
                       
                       
                    </form>

                </div>
                <div id="load" class="hidden">
                    <h4 style="text-align : center">Adding course, please wait...</h4>
                </div>
            </div>
            <div class="modal-footer">
                <a href="javascript:;" class="btn btn-sm btn-white" data-dismiss="modal">Close</a>
                <a href="javascript:;" class="btn btn-sm btn-success" id="addcourse">submit</a>
            </div>
        </div>
    </div>
</div>